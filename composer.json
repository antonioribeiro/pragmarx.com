{
    "name": "laravel/laravel",

    "description": "The Laravel Framework.",

    "keywords": ["framework", "laravel"],

    "license": "MIT",

    "type": "project",

    "require": {
        "php": ">=5.6.4",
        "doctrine/dbal": "^2.6@dev",
        "docusign/esign-client": "^2.0",
        "garygreen/pretty-routes": "^0.0",
        "geoip2/geoip2": "dev-master",
        "guzzlehttp/guzzle": "^6.2",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "pragmarx/ci": "dev-master",
        "pragmarx/countries": "dev-master",
        "pragmarx/firewall": "dev-master",
        "pragmarx/google2fa-laravel": "dev-master",
        "pragmarx/health": "dev-master",
        "pragmarx/tracker": "dev-master",
        "predis/predis": "^1.0"
    },

    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7",
        "laravel/dusk": "^1.1"
    },

    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/antonioribeiro/google2fa-laravel"
        }
    ],

    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
          "app/Support/helpers.php"
        ]
    },

    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },

    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },

    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },

    "minimum-stability": "dev",

    "prefer-stable": true
}
